            </varlistentry>

            <varlistentry>
              <term>Sub elements</term>

              <listitem>
                <para><informaltable>
                    <tgroup cols="5">
                      <thead>
                        <row>
                          <entry align="center"> Name</entry>

                          <entry align="center"> Description</entry>

                          <entry align="center">Type</entry>

                          <entry align="center">Default</entry>

